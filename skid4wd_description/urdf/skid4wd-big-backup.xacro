<!--ULTRASONIC SENSORS (front (right , left) and rear (right, left) and rear and front_mid(left,right)-->
<joint name="sonar_front_right_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin rpy="0 ${mPI_12} ${mPI_4}" xyz="0.58 -0.29 0.16" /> <!--z raised from 5 to 16 bcs of bodywork raise
                                                                      same with all sonar joints-->
    <parent link="base_link"/>
    <child link="base_sonar_front_right"/>
</joint><link name="base_sonar_front_right">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </collision>    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
 </link>

