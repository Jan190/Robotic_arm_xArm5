 <gazebo reference="base_sonar_front_right">        
   <sensor type="ray" name="TeraRanger_fr">
      <pose>0 0 0 0 0 0</pose>
      <visualize>true</visualize>
      <update_rate>50</update_rate>
      <ray>
         <scan>
            <horizontal>
               <samples>10</samples>
               <resolution>1</resolution> 
               <min_angle>${mPI_12}</min_angle>
               <max_angle>${PI_12}</max_angle>
            </horizontal>
            <vertical>
               <samples>10</samples>
               <resolution>1</resolution> 
               <min_angle>${mPI_12}</min_angle>
               <max_angle>${PI_12}</max_angle>
            </vertical>
         </scan>
         <range>
            <min>0.01</min>
            <max>0.4</max>
            <resolution>0.02</resolution>
         </range>
      </ray>
      <plugin filename="libgazebo_ros_range.so" name="gazebo_ros_range">
         <gaussianNoise>0.005</gaussianNoise>
         <alwaysOn>true</alwaysOn>
         <updateRate>50</updateRate>
         <topicName>sensor_msgs/sonar_front_right</topicName>
         <frameName>base_sonar_front_right</frameName>
         <radiation>ULTRASOUND</radiation>
         <fov>0.2967</fov>
      </plugin>
   </sensor>  
   </gazebo>

