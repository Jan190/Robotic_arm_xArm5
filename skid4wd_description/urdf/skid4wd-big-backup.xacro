    <origin rpy="${PI/2} 0 0" xyz="0 0 0"/>
    <geometry>
      <cylinder length="0.1" radius="0.16"/>
    </geometry>
  </collision>
</link>

<!-- 
  Adding robotic hand
  -->
<link name="riser">
  <inertial>
    <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    <mass value="2"/>
    <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
  </inertial>
  <visual>
    <origin rpy="0.0 0.0 0.0" xyz="0.50 0 0.4"/>
    <geometry>
      <box size="0.1 0.1 0.8" />
    </geometry>
    <material name="orange"/>
  </visual>
  <collision>
    <origin rpy="0.0 0.0 0.0" xyz="0.50 0 0.4"/>
    <geometry>
      <box size="0.1 0.1 0.8" />
    </geometry>
</collision>
</link>

<link name="palm">
  <inertial>
    <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
    <mass value="0.05"/>
    <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
  </inertial>
  <visual>
    <origin rpy="0 0 0" xyz="0.56 0 0.10"/>
    <geometry>
      <box size="0.04 0.04 0.08" />
    </geometry>
    <material name="orange"/>
  </visual>
  <collision>
    <origin rpy="0 0 0" xyz="0.56 0 0.10"/>
    <geometry>
      <box size="0.04 0.04 0.08" />
    </geometry>
  </collision>
</link>

<!-- Camera mount link-->

<link name="camera_mount">
  <visual>
    <geometry>
      <cylinder radius="0.04" length="0.2" />
    </geometry>
    <material name="">
    </material>
  </visual>
</link>
<gazebo reference="camera_mount">
  <material>Gazebo/Blue</material>
</gazebo>
<joint name="camera_mount_joint" type="fixed">
  <parent link="base_link" />
  <child link="camera_mount" />
  <origin xyz="0.5 0.0 0.8" rpy="0 0 0" />
</joint>

<!-- camera -->
<xacro:stereo_camera parent="camera_mount" origin="0 0 0.12" /> 


<!-- camera INTEL
<xacro:include filename="$(find skid4wd_description)/urdf/_d435.urdf.xacro" />
<sensor_d435 parent="camera_mount">
</sensor_d435>
-->
   
<!-- 
  JOINTS
-->

<!-- Fixed hand joint -->
<joint name="joint_hand_fix" type="fixed">
  <parent link="base_link"/>
  <child link="riser"/>
</joint>

<joint name="joint_palm_riser" type="prismatic">
  <parent link="riser"/>
  <child link="palm"/>
  <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
  <axis xyz="0 0 1"/>
  <limit effort="10.0" lower="0.0" upper="0.35" velocity="0.5"/>
</joint>

<joint name="joint_front_left" type="continuous"> 
  <origin rpy="0 0 0" xyz="0.30 0.38 -0.04"/>
  <parent link="base_link"/>
  <child link="wheel_front_left_1"/>
  <axis xyz="0.0 1.0 0.0"/>
  <limit effort="2000" velocity="10"/>
  <joint_properties damping="20000.0" friction="2.0"/>
  <cfm>0.0017</cfm> 
  <erp>0.17</erp>
</joint>

<joint name="joint_front_right" type="continuous">
  <origin rpy="0 0 0" xyz="0.30 -0.38 -0.04"/>
  <parent link="base_link"/>
  <child link="wheel_front_right_1"/>
  <axis xyz="0.0 1.0 0.0"/>
  <limit effort="2000" velocity="10"/>
  <joint_properties damping="20000.0" friction="2.0"/>
  <cfm>0.000017</cfm> 
  <erp>0.17</erp>
</joint>

<joint name="joint_rear_left" type="continuous">
  <origin rpy="0 0 0" xyz="-0.320 0.38 -0.04"/>
  <parent link="base_link"/>
  <child link="wheel_rear_left_1"/>
  <axis xyz="0.0 1.0 0.0"/>
  <limit effort="2000" velocity="10"/>
  <joint_properties damping="20000.0" friction="2.0"/>
  <cfm>0.000017</cfm> 
  <erp>0.17</erp>
</joint>

<joint name="joint_rear_right" type="continuous">
  <origin rpy="0 0 0" xyz="-0.320 -0.38 -0.04"/>
  <parent link="base_link"/>
  <child link="wheel_rear_right_1"/>
  <axis xyz="0.0 1.0 0.0"/>
  <limit effort="2000" velocity="10"/>
  <joint_properties damping="20000.0" friction="2.0"/>
  <cfm>0.000017</cfm> 
  <erp>0.17</erp>
</joint>

</robot>
