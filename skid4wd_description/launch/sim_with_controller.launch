<launch>
    <!-- Default: Empty World -->
    <arg name="world_name" default="worlds/empty.world"/>

    <!-- Launch Gazebo -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
    	<!-- arg world 
        <arg name="world_name" value="$(find gazebo_worlds)/worlds/$(arg world).world"/>
        -->
        <arg name="paused" value="false"/>
    </include>

    <!-- Spawn URDF Model -->
    <include file="$(find skid4wd_description)/launch/spawn.launch"/>

    <!-- Spawn Controllers -->
    <include file="$(find skid4wd_description)/launch/controller.launch"/>
    
    <!-- Spawn GPS server 
    <include file="$(find skid4wd_description)/launch/gazebo_GPS_server.launch"/>  -->
	
	<!-- Spawn RTABMAP server
    <include file="$(find skid4wd_description)/launch/rta_bmap.launch"/>-->
	 
</launch>
